        <dcterms:title>Vis voor de toekomst; Smikkelende puppy Tirza brengt baasje op gouden idee</dcterms:title>
        <dcterms:date>2012</dcterms:date>
        <dcterms:publisher>
          <rdf:Description rdf:about="http://data.bibliotheek.nl/lexisnexis/telegraaf">

        <dcterms:title>Women Together/women Apart : Portraits of Lesbian Paris</dcterms:title>
        <dcterms:creator>Latimer, T.</dcterms:creator>
        <dcterms:date>2005</dcterms:date>
        <dcterms:publisher>Rutgers University Press, New Brundwick, NJ</dcterms:publisher>
        <dcterms:language>
